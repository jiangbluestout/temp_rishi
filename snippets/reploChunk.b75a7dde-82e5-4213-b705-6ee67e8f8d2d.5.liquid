<style>
  [data-quick-add-new="true"] {
    opacity: 0;
    visibility: hidden;
    pointer-events: none;
    transition: opacity 0.2s ease, visibility 0.2s ease;
  }

  .show-element {
    opacity: 1 !important;
    visibility: visible !important;
    pointer-events: auto !important;
  }
</style>
<script>
  document.addEventListener("DOMContentLoaded", function () {
    const buyBox = document.querySelector('[data-buy-box-new="true"]');
    const quickAddBtn = document.querySelector('[data-quick-add-new="true"]');

    if (!buyBox || !quickAddBtn) return;

    // Hide on load
    quickAddBtn.classList.remove("show-element");

    // Show on hover
    buyBox.addEventListener("mouseenter", () => {
      quickAddBtn.classList.add("show-element");
    });

    // Hide on mouse leave
    buyBox.addEventListener("mouseleave", () => {
      quickAddBtn.classList.remove("show-element");
    });
