        e.stopPropagation();
        quickAddBtn.classList.remove("show-element");
        variantContainer.classList.add("show-element");
      });

      // Hover Out: Hide both
      buyBox.addEventListener("mouseleave", () => {
        quickAddBtn.classList.remove("show-element");
        variantContainer.classList.remove("show-element");
      });
    }
  });
