<script>
  document.addEventListener("DOMContentLoaded", function () {
    for (let i = 1; i <= 8; i++) {
      const buyBox = document.querySelector(`[data-buy-box-${i}="true"]`);
      const quickAddBtn = document.querySelector(`[data-quick-add-${i}="true"]`);
      const variantContainer = document.querySelector(`[data-variant-container-${i}="true"]`);

      if (!buyBox || !quickAddBtn || !variantContainer) continue;

      // Hover In: Show Quick Add
      buyBox.addEventListener("mouseenter", () => {
        quickAddBtn.classList.add("show-element");
        variantContainer.classList.remove("show-element");
      });

      // Click on Quick Add: Hide button, Show variant container
      quickAddBtn.addEventListener("click", (e) => {
        e.stopPropagation();
        quickAddBtn.classList.remove("show-element");
        variantContainer.classList.add("show-element");
      });

      // Hover Out: Hide both
      buyBox.addEventListener("mouseleave", () => {
        quickAddBtn.classList.remove("show-element");
        variantContainer.classList.remove("show-element");
      });
    }
  });
